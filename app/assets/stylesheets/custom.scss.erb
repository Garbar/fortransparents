@import 'theme';
/*

* =====================
* CUSTOM
* =====================
*/

#header {
  background: #fff;
  border-bottom: 1px rgba(0, 0, 0, 0.07) solid;
  z-index: 99;
  &.--not-top {
    box-shadow: 0 0 30px rgba(0, 0, 0, 0.1);
  }
}

.colorgraph {
  height: 7px;
  border-top: 0;
  background: #c4e17f;
  border-radius: 5px;
  background-image: -webkit-linear-gradient(left, #c4e17f, #c4e17f 12.5%, #f7fdca 12.5%, #f7fdca 25%, #fecf71 25%, #fecf71 37.5%, #f0776c 37.5%, #f0776c 50%, #db9dbe 50%, #db9dbe 62.5%, #c49cde 62.5%, #c49cde 75%, #669ae1 75%, #669ae1 87.5%, #62c2e4 87.5%, #62c2e4);
  background-image: -moz-linear-gradient(left, #c4e17f, #c4e17f 12.5%, #f7fdca 12.5%, #f7fdca 25%, #fecf71 25%, #fecf71 37.5%, #f0776c 37.5%, #f0776c 50%, #db9dbe 50%, #db9dbe 62.5%, #c49cde 62.5%, #c49cde 75%, #669ae1 75%, #669ae1 87.5%, #62c2e4 87.5%, #62c2e4);
  background-image: -o-linear-gradient(left, #c4e17f, #c4e17f 12.5%, #f7fdca 12.5%, #f7fdca 25%, #fecf71 25%, #fecf71 37.5%, #f0776c 37.5%, #f0776c 50%, #db9dbe 50%, #db9dbe 62.5%, #c49cde 62.5%, #c49cde 75%, #669ae1 75%, #669ae1 87.5%, #62c2e4 87.5%, #62c2e4);
  background-image: linear-gradient(to right, #c4e17f, #c4e17f 12.5%, #f7fdca 12.5%, #f7fdca 25%, #fecf71 25%, #fecf71 37.5%, #f0776c 37.5%, #f0776c 50%, #db9dbe 50%, #db9dbe 62.5%, #c49cde 62.5%, #c49cde 75%, #669ae1 75%, #669ae1 87.5%, #62c2e4 87.5%, #62c2e4);
}

@media(min-width: 768px) {
  .field-label-responsive {
    padding-top: .5rem;
    text-align: right;
  }
}

.authform {
  padding-top: 90px;
}

.profile-img-card {
    width: 96px;
    height: 96px;
    margin: 0 auto 10px;
    display: block;
    -moz-border-radius: 50%;
    -webkit-border-radius: 50%;
    border-radius: 50%;
}

main .alert{
  padding-top: 80px;
}
.intro{
  padding-top: 40px;
}
.logo-wrapper {
  min-width: 80px;
  height: 100px;
  overflow: hidden;
}
.icon-boxes{
  padding: 100px 0;
  overflow-x: hidden;
  background: url(<%= asset_path 'transgender_pride_flag2.png' %>) 0 100% repeat-x;
}
.icon-box.ib-v3 > i, .icon-box.ib-v4 > i, .icon-box.ib-v1 > i {
    float: left;
    font-size: 50px;
    line-height: 1;
    color: #4b4b4b;
}
.icon-box.ib-v3 > i:before, .icon-box.ib-v4 > i:before, .icon-box.ib-v1 > i:before {
    vertical-align: bottom;
}
.icon-box.ib-v3 p, .icon-box.ib-v4 p, .icon-box.ib-v1 p {
    line-height: 20px;
}
.dark-wrapper .icon-box.ib-v3 i, .dark-wrapper .icon-box.ib-v4 i, .dark-wrapper .icon-box.ib-v1 i {
    color: #fff;
}
.icon-box.ib-v4 > i {
    font-size: 34px;
}
.icon-box.ib-v3 .title, .icon-box.ib-v4 .title, .icon-box.ib-v1 .title {
    font-size: 18px;
    font-weight: bold;
    font-weight: 700;
    text-transform: capitalize;
    margin-bottom: 25px;
    margin-left: 65px;
    position: relative;
}
.icon-box.ib-v3 .title span, .icon-box.ib-v4 .title span, .icon-box.ib-v1 .title span {
    position: absolute;
    bottom: 0;
    left: 0;
}
.icon-box.ib-v3 .title:after, .icon-box.ib-v4 .title:after, .icon-box.ib-v1 .title:after {
    content: '';
    display: table;
    clear: left;
}
.icon-box.ib-v3 > i, .icon-box.ib-v4 > i, .icon-box.ib-v1 > i {
    float: left;
    font-size: 50px;
    line-height: 1;
    color: #4b4b4b;
}
.icon-box.ib-v3 > i:before, .icon-box.ib-v4 > i:before, .icon-box.ib-v1 > i:before {
    vertical-align: bottom;
}
.icon-box.ib-v3 p, .icon-box.ib-v4 p, .icon-box.ib-v1 p {
    line-height: 20px;
}
.dark-wrapper .icon-box.ib-v3 i, .dark-wrapper .icon-box.ib-v4 i, .dark-wrapper .icon-box.ib-v1 i {
    color: #fff;
}
.icon-box.ib-v4 > i {
    font-size: 34px;
}
.icon-box.ib-v4 p {
    margin-left: 65px;
    line-height: 22px;
}
.icon-box.ib-v4 .title {
    margin-bottom: 15px;
    padding-top: 7px;
    font-size: 16px;
}
.icon-box.ib-v4 .title span {
    position: static;
    display: block;
}
.icon-box.ib-v4 .title.with-shaded-label {
    padding-top: 17px;
}
.icon-box.ib-v4 .title.with-shaded-label .shaded-label {
    bottom: 2px;
    top: auto;
}
.with-shaded-label .shaded-label {
    position: absolute !important;
    top: 0px;
    left: 0;
    z-index: -1;
    font-size: 220%;
    color: #dedede;
}

.page-head {
    z-index: 1;
    height: 150px;
    background-size: cover;
    position: relative;
    font-family: "Play",sans-serif;/*	skins	*/
    color: #636363;
    background-color: #f5f5f5;/* why???*/
}
.page-head .container {/* overflow:hidden; */
    height: 100%;
    z-index: 10;
}
.page-head.lined {
    border-bottom: 1px solid #dedede;
}
.tb-vcenter-wrapper {
    position: relative;
    display: table;
    width: 100%;
    height: 100%;
}
.page-head.lined .vcenter {
    vertical-align: bottom;
    padding-bottom: 20px;
}
.page-head.lined .vcenter.title-wrapper:after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    height: 3px;
    width: 50px;
    background-color: #4b4b4b;
}
.page-head .title-wrapper .info-wrapper {
    padding-top: 20px;
    padding-bottom: 20px;
    line-height: 20px;
    font-family: 'Sintony',sans-serif;
}
.page-head .breadcrumb-wrapper {
    width: 50%;
    text-align: right;
}
.tb-vcenter-wrapper .vcenter {
    display: table-cell;
    float: none;
}
.page-head h1.title {
    font-size: 26px;
    font-weight: 700;
    text-transform: capitalize;
    letter-spacing: 1px;
}

/*	Breadcrumbs
 *	===================================================
 * 
 */

.breadcrumbs-wrapper{
    padding: 150px 0 15px 0;
    background-color: #e8e8e8;
 }
 .breadcrumbs-wrapper ul {
    line-height: 1.5;
}
.breadcrumb {
    margin: 0;
    padding: 0;
    color: #636363;
    font-size: 15px;
    text-transform: capitalize;
    background: transparent;
    border-radius: 4px;
    font-family: 'Sintony',sans-serif;
}
.breadcrumb li:first-of-type {
    padding-left: 0;
}
.breadcrumb > li {
    position: relative;
    padding: 0 12px;
    display: inline-block;
}
.breadcrumb > li:last-child {
    padding-right: 0;
}
.breadcrumb a {
    color: #636363;
    text-decoration: none;
}

/*	Contact
 *	===================================================
 *
 */

 /*------ Custom Column -------*/
.col-xxs-1,.col-xxs-10,.col-xxs-11,.col-xxs-12,.col-xxs-2,.col-xxs-3,.col-xxs-4,.col-xxs-5,.col-xxs-6,.col-xxs-7,.col-xxs-8,.col-xxs-9 {
	position:relative;
	min-height:1px;
	padding-right:15px;
	padding-left:15px;
	float:left
}
.pt-100 {
    padding-top: 100px;
}
.pb-100 {
    padding-bottom: 100px
}
.contact_page_2 .col-md-6 {
    float: left;
}
.contact_page_2 {
    font-family: 'Roboto', sans-serif;
}
.contact_address {
    width: 150px
}
.contact_address h6 { 
    color: #5c6672;
    font-size: 16px;
    line-height: 30px;
    font-weight: 400
}
.contact__phone {
    vertical-align: top;
    float: right
}
.contact__phone a,
.contact__phone i {
    color: #5c6672;
    font-size: 15px;
    font-weight: 400;
    margin-bottom: 15px
}

.contact__phone i {
    margin-right: 10px
} 
.contact_page_2 .contact__info {
    padding-top: 35px;
}
.contact__info p {
    color: #5c6672;
    font-size: 16px;
    border-top: 1px solid #ddd;
    padding-top: 40px;
    margin-top: 30px;
    font-weight: 400
}
.contact_form_page label {
    color: #5c6672;
    font-size: 16px;
    font-weight: 400;
    margin-bottom: 10px
}
.contact_form_page input {
    background: #fcfcfc;
    border: 1px solid #dddddd;
    border-radius: 3px;
    height: 45px;
    padding-left: 10px;
    margin-bottom: 20px;
    width: 100%
}
.wpcf7-form-control {
    padding: 5px 20px;
    outline: 0;
    resize: vertical;
    box-shadow: none
}
.contact_form_page textarea {
    background: #fcfcfc;
    border: 1px solid #ddd;
    border-radius: 3px;
    height: 165px;
    margin-bottom: 32px;
    padding-left: 10px;
    width: 100%
}
.wpcf7-form-control {
    outline: 0;
    resize: vertical;
    box-shadow: none
}

.btn.box-s-none {
    -webkit-box-shadow: none;
    box-shadow: none;
    background: #ffc31f;
    color: #23262e;
    font-size: 13px;
    min-width: 140px;
    height: 40px;
    line-height: 40px;
    letter-spacing: .3px;
    font-weight: 500;
    border-radius: 0px;
    text-transform: uppercase;
    border: none;
    padding-left: 20px;
    padding-right: 20px;
    padding-top: 0;
    padding-bottom: 0;
}

.contact_title h1 {
    color: #222;
    font-weight: 500;
    margin-bottom: 15px
}

.contact_title hr {
    border-bottom: 4px solid #222222;
    width: 220px;
    margin: 0 auto
}
.visible-xxs {
	display:none!important
}
